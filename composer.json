{
    "name": "alecrabbit/accessories",
    "description": "PHP accessories Classes etc.",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "/var/packages/simple-profiler"
        },
        {
            "type": "vcs",
            "url": "/var/packages/helpers"
        }
    ],
    "require": {
        "php": ">=7.2",
        "alecrabbit/php-helpers": "dev-master",
        "alecrabbit/simple-profiler": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "nunomaduro/collision": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AlecRabbit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "prefer-stable":true
}
