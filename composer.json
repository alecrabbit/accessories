    "description": "PHP accessories Classes etc.",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "alecrabbit/php-helpers": "^0"
    },
    "require-dev": {
        "nunomaduro/collision": "^2.0",
        "phpstan/phpstan": "^0.10.5",
